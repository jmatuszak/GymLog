@using GymLog.ViewModels
@model SetVM



<form method="post" asp-action="Edit">
    <input type="hidden" value="Id" id="Id" />
    <div class="form-group">
        <label asp-for="Weight">Weight</label>
        <input asp-for="Weight" class="form-control" placeholder="Weight">
        <span asp-validation-for="Weight" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="Reps">Reps</label>
        <input asp-for="Reps" class="form-control" placeholder="Reps">
        <span asp-validation-for="Reps" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="SetCollectionId">SetCollectionId</label>
        <input asp-for="SetCollectionId" class="form-control" placeholder="SetCollectionId">
        <span asp-validation-for="SetCollectionId" class="text-danger"></span>

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>