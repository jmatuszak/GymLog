@using GymLog.ViewModels
@model Set



<form method="post" asp-action="Create">
    <div class="form-group">
        <label asp-for="Weight">Weight</label>
        <input asp-for="Weight" class="form-control" placeholder="Weight">
        <span asp-validation-for="Weight" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="Reps">Reps</label>
        <input asp-for="Reps" class="form-control" placeholder="Reps">
        <span asp-validation-for="Reps" class="text-danger"></span>

    </div>
    <div class="form-group">
        <label asp-for="WorkoutSegmentId">WorkoutSegmentId</label>
        <input asp-for="WorkoutSegmentId" class="form-control" placeholder="WorkoutSegmentId">
        <span asp-validation-for="WorkoutSegmentId" class="text-danger"></span>

    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>